{
	"name": "ocaml-scala-run",
	"displayName": "ocaml-scala-run",
	"description": "Run OCaml and Scala scripts in REPL with a single click.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ocaml-scala-run.runScala",
		"onCommand:ocaml-scala-run.runOCaml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ocaml-scala-run.runOCaml",
				"title": "Run in OCaml REPL"
			},
			{
				"command": "ocaml-scala-run.runScala",
				"title": "Run in Scala REPL"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}